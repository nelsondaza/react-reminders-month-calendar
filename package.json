  ],
  "scripts": {
    "build": "rm -rf ./dist/public && yarn compile",
    "compile": "yarn install && yarn cross-env NODE_ENV=production webpack --env.env=prod -p --bail",
    "deploy": "gh-pages -d ./dist/public",
    "lint": "yarn eslint ./src",
    "predeploy": "yarn build",
    "serve": "yarn install && yarn webpack-dev-server --env.env=dev --cache --bail",
    "stories": "yarn install && yarn test && yarn start-storybook -p 7074 -s ./static -c config/storybook --ci",
    "test": "jest --config=config/jest/index.js -u --json --outputFile=./dist/coverage/test-results.json",
    "test:w": "yarn test --watchAll"
  },
